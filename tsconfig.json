{
    "compilerOptions": {
        "charset": "utf8",
        "forceConsistentCasingInFileNames": true,
        "newLine": "lf",
        "noFallthroughCasesInSwitch": false,
        "noImplicitReturns": false,
        "noUncheckedIndexedAccess": false,
        "noUnusedParameters": false,
        "strict": false,
        "module": "es6",
        "target": "es5",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noImplicitAny": false
    }
}
